{
  "plugins": [
    "js-sequence-diagram-full@>=0.3.1",
    "prism",
    "-highlight",
    "anchors",
    "splitter",
    "github",
    "github-buttons",
    "anchor-navigation-ex",
    "alerts",
    "expandable-chapters-small",
    "favicon",
    "pageview-count",
    "mermaid-gb3"
  ],
  "pluginsConfig": {
    "prism": {
      "css": [
        "prismjs/themes/prism-okaidia.css"
      ],
      "lang": {
        "java": "java"
      },
      "ignore": [
        "sequence"
      ]
    },
    "github": {
      "url": "https://github.com/TuyaInc/tuyasmart_lock_android_sdk"
    },
    "github-buttons": {
      "buttons": [{
        "user": "TuyaInc",
        "repo": "tuyasmart_lock_android_sdk",
        "type": "star",
        "size": "small"
      },{
        "user": "TuyaInc",
        "repo": "tuyasmart_lock_android_sdk",
        "type": "watch",
        "size": "small"
      },{
        "user": "TuyaInc",
        "type": "follow",
        "size": "small",
        "count": true
      }]
    },
    "sharing": {
      "facebook": true,
      "google": true,
      "twitter": true,
      "weibo": true,
      "all": ["facebook", "google", "twitter", "weibo"]
    },
    "anchor-navigation-ex": {
      "showLevel": false,
      "associatedWithSummary": false,
      "showGoTop": true
    },
    "favicon": {
            "shortcut": "zh-hans/resource/images/bitbug_favicon.ico"
        }
  }
}